types WikiPages {
	type main_page = main_wiki_page {
		state = {
			name = wiki_main_page
			on_start = "[PdxGuiWidget.Show]"
			delay = 0.001
		}
		textbox = {
			size = { 100 100 }
			margin_left = 5
			parentanchor = left
			align = top
			block "TextBox" { text = "text" }
			multiline = yes
			using = BaseFontM
			using = Black_FontColor
		}
	}
### UNITS
	type conscripts_page = units_wiki_page {
		state = {
			name = conscripts_page
			on_start = "[PdxGuiWidget.Show]"
			delay = 0.001
		}
		blockoverride "Title_text" { text = "conscripts" }
		blockoverride "Image" { texture = "gfx/interface/wiki/units/conscripts.dds" }
		blockoverride "Cost" { text = "5" }
		blockoverride "Manpower" { text = "500" }
		#blockoverride "Type" { text = "Infantry" }
		blockoverride "Assault" { text = "yes" }
		blockoverride "Time" { text = "45" }
		blockoverride "Maneuver" { text = "1" }
		blockoverride "Speed" { text = "2.4" }
		blockoverride "TextBox" { text = "CONSCRIPTS_DESC" }
		blockoverride "Attrition_weight_value" { text = "-10%" using = Positive_FontColor }
		blockoverride "Attrition_loss_value" { text = "7.00%" using = Negative_FontColor }
		blockoverride "Food_consumption_value" { text = "0.1" using = Negative_FontColor }
		blockoverride "Food_storage_value" { text = "2.2" using = Positive_FontColor }
		blockoverride "Morale_damage_taken_value" { text = "+30%" using = Negative_FontColor }
	}
